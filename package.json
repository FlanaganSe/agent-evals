{
	"name": "agent-evals",
	"version": "0.0.1",
	"description": "TypeScript-native eval framework for AI agent workflows. Record-replay, deterministic + LLM graders, trajectory evaluation.",
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"bin": {
		"agent-evals": "./bin/cli.mjs"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./graders": {
			"types": "./dist/graders/index.d.ts",
			"default": "./dist/graders/index.js"
		},
		"./comparison": {
			"types": "./dist/comparison/compare.d.ts",
			"default": "./dist/comparison/compare.js"
		},
		"./plugin": {
			"types": "./dist/plugin/types.d.ts",
			"default": "./dist/plugin/types.js"
		},
		"./reporters": {
			"types": "./dist/reporters/types.d.ts",
			"default": "./dist/reporters/types.js"
		},
		"./fixtures": {
			"types": "./dist/fixtures/index.d.ts",
			"default": "./dist/fixtures/index.js"
		},
		"./watcher": {
			"types": "./dist/watcher/index.d.ts",
			"default": "./dist/watcher/index.js"
		}
	},
	"files": [
		"dist",
		"bin"
	],
	"sideEffects": false,
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf dist",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit",
		"prepare": "lefthook install",
		"verify": "pnpm typecheck && pnpm lint && pnpm test",
		"validate-package": "pnpm build && npx publint && npx @arethetypeswrong/cli --pack . --profile esm-only"
	},
	"keywords": [
		"ai",
		"agent",
		"eval",
		"evaluation",
		"testing",
		"llm",
		"grader",
		"record-replay"
	],
	"license": "MIT",
	"dependencies": {
		"@clack/prompts": "^1.0.1",
		"c12": "^3.3.3",
		"chokidar": "^4.0.3",
		"citty": "^0.2.1",
		"consola": "^3.4.2",
		"jiti": "^2.6.1",
		"picocolors": "^1.1.1",
		"yaml": "^2.8.2",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@anthropic-ai/sdk": "^0.78.0",
		"@biomejs/biome": "^2.4.4",
		"@modelcontextprotocol/sdk": "^1.27.1",
		"@types/node": "^25.3.2",
		"lefthook": "^2.1.1",
		"openai": "^6.25.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"peerDependencies": {
		"@modelcontextprotocol/sdk": "^1.20.0"
	},
	"peerDependenciesMeta": {
		"@modelcontextprotocol/sdk": {
			"optional": true
		}
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"@biomejs/biome",
			"lefthook"
		]
	}
}
